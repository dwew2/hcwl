<!--
<!ENTITY % x.refsys 'lbh |'>
<!ENTITY % x.edit 'dsp | isp|'>
-->
<!ENTITY dsp "&#x005F;">
<!-- dsp="delete space", i.e., the manuscript has a space which, in regularisation, should be deleted; the standard replacement string is understroke; leave empty for the regularised version -->
<!ENTITY isp "&#x007C;">
<!-- isp = "insert space", i.e., the manuscript has no space where, in regularisation, a space should be inserted; here represented by vertical bar; give "&#x00A0" = nbsp for the regularised version-->
<!ENTITY insmark "&#x0022;">
<!-- quotation mark as insertion mark -->
<!ENTITY shy "-">
